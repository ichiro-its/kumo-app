{
  "name": "kumo-app",
  "version": "0.1.0",
  "description": "React wrapper for Kumo client library",
  "keywords": [
    "javascript",
    "kumo",
    "kumo-client",
    "material-ui",
    "provider",
    "react",
    "ros",
    "ros2",
    "websocket"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "repository": "git@github.com:ichiro-its/kumo-app.git",
  "scripts": {
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore ."
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "kumo-client": "^0.2.1",
    "notistack": "^1.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "store2": "^2.12.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  }
}
